{
    "name": "talesoft/tale-jade",
    "type": "library",
    "keywords": ["template", "dialect", "html", "views", "presentation", "jade", "html", "php", "phtml"],
    "description": "A complete and fully-functional implementation of the Jade template language for PHP",
    "license": "MIT",
    "homepage": "http://jade.talesoft.io",
    "authors": [
        {
            "name": "Talesoft",
            "homepage": "http://talesoft.io",
            "email": "info@talesoft.io"
        },
        {
            "name": "Torben Koehn",
            "email": "tk@talesoft.io"
        }
    ],
    "support": {
        "email": "info@talesoft.io",
        "issues": "https://github.com/talesoft/tale-jade/issues",
        "source": "https://github.com/talesoft/tale-jade",
        "docs": "http://jade.talesoft.io/docs"
    },
    "require": {
        "php": ">=5.5.0",
        "talesoft/tale-config": "0.2",
        "talesoft/tale-factory": "0.1",
        "talesoft/tale-tree": "0.1",
        "talesoft/tale-reader": "0.1"
    },
    "require-dev": {
        "twig/twig": "~1.12|~2.0",
        "oyejorge/less.php": "~1",
        "leafo/scssphp": "~0.3",
        "coffeescript/coffeescript": "~1",
        "michelf/php-markdown": "~1.3",
        "erusev/parsedown": "~1",
        "league/commonmark": ">=0.9",
        "cebe/markdown": "~1",
        "gregwar/rst": "~1",
        "neemzy/stylus": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Tale\\": ""
        }
    },
    "bin": ["tale-jade"],
    "suggest": {
        "erusev/parsedown": "Enables Markdown filtering",
        "coffeescript/coffeescript": "Enables CoffeeScript filtering",
        "leafo/lessphp": "Enables LESS filtering",
        "neemzy/stylus": "Enables Stylus filtering",
        "leafo/scssphp": "Enables SASS filtering"
    }
}
